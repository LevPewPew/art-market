<header class="header">
  <div class="company">
    <div class="name">
      <h1>Art Haus</h1>
    </div>
    <div class="logo">
      <h1>|SOME_LOGO|</h1>
    </div>
  </div>
  <div class="links">
    <% if user_signed_in? %>
      <%= link_to "Settings", user_settings_path %>
      <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
    <% else %>
      <%= link_to "Sign In/Up", new_user_session_path %>
    <% end %>
  </div>
</header>